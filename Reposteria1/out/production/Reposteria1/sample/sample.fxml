<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="500.0" prefWidth="500.0" spacing="20.0" styleClass="fondo" stylesheets="@estilos.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Label text="REPOSTERIA" textFill="#cd7a7a">
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin>
         <font>
            <Font name="MS Gothic" size="35.0" />
         </font>
      </Label>
      <TextField maxWidth="250.0" prefHeight="30.0" promptText="Usuario" styleClass="textAceptar" />
      <PasswordField maxWidth="250.0" prefHeight="30.0" promptText="Password" styleClass="textAceptar" />
      <Button maxWidth="200.0" mnemonicParsing="false" prefHeight="30.0" styleClass="buttonAceptar" text="Ingresar" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
